<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" style="-fx-background-color: #434343;" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.eldarian.fx.ServerConfigController">
   <children>
      <TextField fx:id="hostTextField" layoutX="7.0" layoutY="34.0" prefHeight="31.0" prefWidth="157.0" promptText="host" />
      <TextField fx:id="portTextField" layoutX="169.0" layoutY="34.0" prefHeight="31.0" prefWidth="49.0" promptText="port" />
      <Button fx:id="connectButton" style="-fx-background-color: #434343;" textFill="WHITE" layoutX="225.0" layoutY="34.0" mnemonicParsing="false" onAction="#connectHandler" text="Connect" />
      <Button fx:id="disconnectButton" style="-fx-background-color: #434343;" textFill="WHITE" layoutX="308.0" layoutY="34.0" mnemonicParsing="false" onAction="#disconnectHandler" text="Disconnect" />

      <Label fx:id="statusBar" style="-fx-background-color: #434343;" layoutX="10.0" layoutY="365.0" prefHeight="21.0" prefWidth="590.0" text="State" />
   </children>
</AnchorPane>
