<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.geometry.*?>
<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<?import org.jfree.chart.fx.ChartViewer?>
<VBox alignment="CENTER" prefHeight="407.0" prefWidth="587.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2" fx:controller="com.eldarian.PrimaryController">
  <children>
    <Label text="Linechart View" />
    <ChartViewer fx:id="lineChartViewer" prefHeight="218.0" prefWidth="547.0" />
    <Label text="Get Data from File" />
    <HBox prefHeight="32.0" prefWidth="547.0">
      <children>
        <TextField prefWidth="466.0" fx:id="filePath" promptText="Insert path to file" />
        <Button fx:id="getDataButton" onAction="#getFromFile" text="Get Data" />
      </children>
    </HBox>
    <Button fx:id="primaryButton" onAction="#switchToSecondary" text="Switch to Secondary View" />
  </children>
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
  </padding>
</VBox>
