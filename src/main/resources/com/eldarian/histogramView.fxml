<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.layout.VBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Button?>
<?import javafx.geometry.Insets?>

<?import org.jfree.chart.fx.ChartViewer?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.control.Tooltip?>
<VBox alignment="CENTER" spacing="20.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.eldarian.HistogramController">
        <Label text="Calibration" />
        <ChartViewer fx:id="histogramChartViewer" prefHeight="200.0" prefWidth="300.0"> </ChartViewer>
        <ToolBar minWidth="500.0" prefHeight="41.0" prefWidth="933.0" VBox.vgrow="ALWAYS">
                <Button mnemonicParsing="false" text="Start">
                    <tooltip>
                        <Tooltip text="Начать отображение" />
                    </tooltip>
                </Button>
                <Button mnemonicParsing="false" text="Freeze" />
                <Button onAction="#switchToCalibrationView" prefWidth="104.0" text="Calibration" />
                <Button alignment="CENTER_RIGHT" onAction="#openSettingsWindow" mnemonicParsing="false" text="Settings..." />
            <VBox.margin>
                <Insets />
            </VBox.margin>
        </ToolBar>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
</VBox>
