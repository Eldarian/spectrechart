<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import org.jfree.chart.fx.ChartViewer?>
<VBox alignment="CENTER" minHeight="600.0" minWidth="800.0" prefHeight="407.0" prefWidth="1024.0" spacing="20.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.eldarian.CalibrationController">
  <children>
    <Label text="Histogram" />
    <ChartViewer fx:id="lineChartViewer" maxHeight="1.7976931348623157E308" maxWidth="1.7976931348623157E308" prefHeight="482.0" prefWidth="984.0" VBox.vgrow="ALWAYS" />
    <ToolBar minWidth="500.0" prefHeight="41.0" prefWidth="933.0" VBox.vgrow="ALWAYS">
      <items>
        <Button mnemonicParsing="false" text="Start">
          <tooltip>
            <Tooltip text="Начать отображение" />
          </tooltip>
        </Button>
        <Button mnemonicParsing="false" text="Freeze" />
        <Button fx:id="calibrationButton" onAction="#switchToHistogram" prefWidth="104.0" text="Histogram" />
        <MenuButton fx:id="channelsChooser" mnemonicParsing="false" text="Channels" textOverrun="CLIP">
          <items>
            <CheckMenuItem mnemonicParsing="false" text="Channel1" />
            <CheckMenuItem mnemonicParsing="false" text="Channel2" />
            <CheckMenuItem mnemonicParsing="false" text="Channel3" />
            <CheckMenuItem mnemonicParsing="false" text="Channel4" />
            <CheckMenuItem mnemonicParsing="false" text="Channel5" />
            <CheckMenuItem mnemonicParsing="false" text="Channel6" />
            <CheckMenuItem mnemonicParsing="false" text="Channel7" />
            <CheckMenuItem mnemonicParsing="false" text="Channel8" />
            <CheckMenuItem mnemonicParsing="false" text="Channel9" />
            <CheckMenuItem mnemonicParsing="false" text="Channel10" />
            <CheckMenuItem mnemonicParsing="false" text="Channel11" />
            <CheckMenuItem mnemonicParsing="false" text="Channel12" />
            <CheckMenuItem mnemonicParsing="false" text="Channel13" />
            <CheckMenuItem mnemonicParsing="false" text="Channel14" />
            <CheckMenuItem mnemonicParsing="false" text="Channel15" />
            <CheckMenuItem mnemonicParsing="false" text="Channel16" />
            <CheckMenuItem mnemonicParsing="false" text="Channel17" />
            <CheckMenuItem mnemonicParsing="false" text="Channel18" />
          </items>
        </MenuButton>
            <Button onAction="#openSettingsWindow" alignment="CENTER_RIGHT" mnemonicParsing="false" text="Settings..." />
      </items>
      <VBox.margin>
        <Insets />
      </VBox.margin>
    </ToolBar>
  </children>
  <padding>
    <Insets top="20.0" />
  </padding>
</VBox>
